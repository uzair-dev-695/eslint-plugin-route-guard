{
  "name": "eslint-plugin-route-guard",
  "version": "0.1.1",
  "description": "ESLint plugin to detect duplicate and conflicting routes across Express, Fastify, and NestJS",
  "keywords": [
    "eslint",
    "plugin",
    "eslint-plugin",
    "routes",
    "routing",
    "express",
    "fastify",
    "nestjs",
    "duplicate-detection",
    "linting"
  ],
  "author": "Uzair Kazi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uzair-dev-695/eslint-plugin-route-guard.git"
  },
  "bugs": {
    "url": "https://github.com/uzair-dev-695/eslint-plugin-route-guard/issues"
  },
  "homepage": "https://github.com/uzair-dev-695/eslint-plugin-route-guard#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "eslint": "^8.0.0 || ^9.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.16",
    "@typescript-eslint/rule-tester": "^8.55.0",
    "@typescript-eslint/utils": "^8.0.0",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^9.0.0",
    "rollup": "^4.9.6",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist coverage",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build"
  }
}
